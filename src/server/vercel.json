{
  "version": 2,
  "builds": [
    {
      "src": "vercel/api/index.js",
      "use": "@vercel/node",
      "config": {
        "prebuild": "cd pushoo && pnpm install && pnpm run build && cd ../vercel && yarn install"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel/api/index.js"
    }
  ]
}